  "scripts": {
    "start": "API_URL=http://localhost:3100 node scripts/start.js",
    "build": "API_URL=/api/ node scripts/build.js",
    "test": "NODE_ENV=test BABEL_DISABLE_CACHE=1 nyc ava",
    "report": "nyc report --reporter=html",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint ./**/*.js || true"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "source": [
      "src/**/*.{js,jsx}",
      "!dist/**/*"
    ],
    "concurrency": 5,
    "failFast": false,
    "failWithoutAssertions": false,
    "tap": true,
    "powerAssert": false,
    "require": [
      "babel-register",
      "ignore-styles"
    ],
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "ignore": "test.js",
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "eslintConfig": {
    "extends": "react-app",
    "parserOptions": {
      "ecmaVersion": 6
    }
  }
}
